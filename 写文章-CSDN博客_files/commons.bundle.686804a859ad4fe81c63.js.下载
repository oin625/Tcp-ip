function noticeAlert(loadElement, isspoof) {
    var noticeBox = $(".notice-box"),
        icon = noticeBox.find("i.notice-icon"),
        pacman = isspoof ? noticeBox.find("div.pacman") : null,
        _this = this,
        _isspoof = isspoof,
        maskTrans = null,
        confirmBox = $('.confirm-box'),
        confirmMask = $('.confirm-mask');
    if (!confirmBox.length) {
        confirmBox = $('<div class="confirm-box"><p class="confirm-title"></p><div class="confirm-button-box"><div class="confirm-button false"></div><div class="confirm-button true"></div></div></div>');
        confirmMask = $('<div class="confirm-mask"></div>');
        $("body").append(confirmMask).append(confirmBox);
    }
    if (loadElement && !noticeBox.length) {
        if (isspoof) {
            noticeBox = $('<div class="notice-box"><i class= "mr8 notice-icon type-success"></i><div class="pacman hide"><div></div><div></div><div></div><div></div><div></div></div><span class="notice"></span></div >');
        } else {
            noticeBox = $('<div class="notice-box"><i class="mr8 notice-icon" style="display: none;"></i><span class="notice"></span></div>');
        }
        $("body").append(noticeBox);
        icon = noticeBox.find("i.notice-icon");
        pacman = isspoof ? noticeBox.find("div.pacman") : null;
    }
    var setTxt = function (txt) {
        if (txt) {
            noticeBox.find("span.notice").text(txt);
        }
    };
    this.success = function (txt, time) {
        !!pacman && pacman.addClass('hide');
        icon.removeClass("type-error type-loading").addClass("type-success").show();
        setTxt(txt);
        _this.show(undefined, time);
    };
    this.error = function (txt) {
        !!pacman && pacman.addClass('hide');
        icon.removeClass("type-success type-loading").addClass("type-error").show();
        setTxt(txt);
        _this.show();
    };
    this.loading = function (ismask, txt) {
        //ismask:
        if (ismask) {
            maskTrans = $("<div class='mask-trans'></div>");
            $("body").append(maskTrans);
        }
        if (!txt || typeof txt !== "string") {
            txt = "";
        }
        if (_isspoof) {
            icon.hide();
            !!pacman && pacman.removeClass('hide');
        } else {
            !!pacman && pacman.addClass('hide');
            icon.removeClass("type-success type-error").addClass("type-loading").show();
        }
        setTxt(txt);
        _this.show(false);
        var hideLoad = function () {
            _this.hide();
        };
        return hideLoad; //
    };
    // this.removeLoad = function(){
    //     if (maskTrans !== null) {
    //         maskTrans.remove();
    //     }
    //     _this.hide();
    // }
    //
    this.show = function (autohide, time) {
        noticeBox.show().animate({
            bottom: '56px'
        }, {
            duration: 200,
            done: function () {
                if (autohide === undefined || autohide) {
                    setTimeout(function () {
                        if (maskTrans !== null) {
                            maskTrans.remove();
                        }
                        noticeBox.fadeOut(500);
                    }, time || 1500);
                }
            }
        });
    };
    this.hide = function () {
        noticeBox.stop().fadeOut(500, function () {
            noticeBox.css({
                bottom: '-56px'
            });
        });
        if (maskTrans !== null) {
            maskTrans.remove();
        }
    };
    /**
     * 
     * @param  {object} event             jquery jquery
     * @param  {string|function} msg      
     * @param  {function} successcallback 
     * @param  {function} errorcallback   
     *
     *   $('.del').click({msg:"",successcallback:deleteArticle},notice.confirm)
     */
    this.confirm = function (event, msg, successcallback, errorcallback) {
        var that = this;
        var locaErrorCallback = typeof errorcallback == 'function' ? errorcallback : '';
        var locasuccessCallback = typeof successcallback == 'function' ? successcallback : '';
        var locamsg = typeof msg == 'function' ? msg.call(that) : msg;
        if (typeof event.data !== 'undefined') {
            var locaErrorCallback = typeof event.data.errorcallback == 'function' ? event.data.errorcallback : '';
            var locasuccessCallback = typeof event.data.successcallback == 'function' ? event.data.successcallback : '';
            var locamsg = typeof event.data.msg == 'function' ? event.data.msg.call(that) : event.data.msg;
        }
        confirmBox.find('.confirm-title').text(locamsg).parent().show();
        confirmMask.show();
        confirmBox.find('.true').click(function () {
            typeof locasuccessCallback == 'function' && locasuccessCallback.call(that);
        });
        confirmBox.find('.false').click(function () {
            typeof locaErrorCallback == 'function' && locaErrorCallback.call(that);
        });
        confirmBox.find('.confirm-button').click(function () {
            confirmBox.fadeOut();
            confirmMask.hide();
        });
    };
}
