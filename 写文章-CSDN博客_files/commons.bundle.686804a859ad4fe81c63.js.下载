  var notice = new __WEBPACK_IMPORTED_MODULE_0_JS_lib_notice_alert_js__["noticeAlert"](true, true);
  $('.del').click(function () {
    var tipString = "";
    if ($(this).is('.drafts')) {
      tipString = ",,";
    }
    if (confirm(tipString)) {
      notice.loading(true, '');
      var articleId = $(this).parent().data('aid');
      var deep = false;
      $.ajax({
        url: blog_address + '/pheapi/article/delete',
        type: 'post',
        data: { articleId: articleId, deep: deep },
        xhrFields: {
          withCredentials: true
        },
        contentType: "application/x-www-form-urlencoded",
        success: function (data) {
          if (data.result == 1) {
            notice.success(data.content);
            setTimeout(function () {
              location.reload();
            }, 500);
          } else {
            notice.error(data.content);
          }
        },
        error: function (err) {
          notice.error('');
        }
      });
    }
  });
  // $('.del').click({msg:"",successcallback:deleteArticle},notice.confirm)
  $('.deepdel').click(function () {
    if (confirm(",,")) {
      notice.loading(true, '');
      var articleId = $(this).parent().data('aid');
      var deep = true;
      $.ajax({
        url: blog_address + '/pheapi/article/delete',
        type: 'post',
        data: { articleId: articleId, deep: deep },
        xhrFields: {
          withCredentials: true
        },
        contentType: "application/x-www-form-urlencoded",
        success: function (data) {
          if (data.result == 1) {
            notice.success(data.content);
            setTimeout(function () {
              location.reload();
            }, 500);
          } else {
            notice.error(data.content);
          }
        },
        error: function (err) {
          notice.error('');
        }
      });
    }
  });

  $('.recycle').click(function () {
    notice.loading(true, '');
    var articleId = $(this).parent().data('aid');
    $.ajax({
      url: blog_address + '/pheapi/article/recycle',
      type: 'post',
      data: { articleId: articleId },
      xhrFields: {
        withCredentials: true
      },
      contentType: "application/x-www-form-urlencoded",
      success: function (data) {
        if (data.result == 1) {
          notice.success(data.content);
          setTimeout(function () {
            location.reload();
          }, 500);
        } else {
          notice.error(data.content);
          console.log(data);
        }
      },
      error: function (err) {
        notice.error('');
        console.log(err);
      }
    });
  });

  function topArticle() {
    var optType = $(this).data('opt');
    var req_url = '/pheapi/article/top';
    if (!optType) {
      req_url = '/pheapi/article/untop';
    }
    // if (confirm(msg)) {
    var artiId = $(this).parent().data('aid');
    notice.loading(true, '');
    $.ajax({
      url: blog_address + req_url,
      type: 'post',
      data: { articleId: artiId },
      dataType: 'json',
      xhrFields: {
        withCredentials: true
      },
      contentType: "application/x-www-form-urlencoded",
      success: function (data) {
        if (data.result == 1) {
          notice.success(data.content);
          setTimeout(function () {
            location.reload();
          }, 500);
        } else {
          notice.error(data.content);
        }
      },
      error: function (err) {
        notice.error('');
      }
    });
    // }
  }
  $('.managecomment').click(function () {
    var optType = $(this).data('opt');
    var aid = $(this).data('aid');
    var text = $(this).data('val');
    var req_url = '/postlist/allowComment';
    $.ajax({
      url: req_url,
      type: 'post',
      data: { id: aid, t: optType },
      datatype: 'json',
      xhrFields: {
        withCredentials: true
      },
      contentType: "application/x-www-form-urlencoded",
      success: function (data) {
        notice.success('');
        $('#' + aid).text(text);
        if (data.result == 1) {
          location.reload();
        }
      },
      error: function (err) {
        notice.error('');
      }
    });
  });

  function getTopMsg() {
    var optType = $(this).data('opt');
    var msg = '';
    if (!optType) {
      msg = '';
    }
    return msg;
  }
  function getUserCard() {
    var article = $(this).data('text');
    var msg = '' + article + "";
    return msg;
  }
  function useArticleCard() {
    var optType = $(this).data('opt');
    var artiId = $(this).parent().data('aid');
    if (optType) {
      notice.loading(true, '');
      $.ajax({
        url: blog_address + 'pheapi/recommend/recommendArticleByCard',
        type: 'post',
        data: { article_id: artiId },
        dataType: 'json',
        xhrFields: {
          withCredentials: true
        },
        contentType: "application/x-www-form-urlencoded",
        success: function (data) {
          if (data.status == 1) {
            notice.success(data.data);
            setTimeout(function () {
              location.reload();
            }, 500);
          } else {
            notice.error(data.msg);
            setTimeout(function () {
              location.reload();
            }, 500);
          }
        },
        error: function (err) {
          notice.error('');
        }
      });
    }
  }
  $('.setTop').click({ msg: getTopMsg, successcallback: topArticle }, notice.confirm);
  $('.useCard').click({ msg: getUserCard, successcallback: useArticleCard }, notice.confirm);
});
